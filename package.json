{
  "name": "justified-opinionated-boilerplate",
  "author": "David Durst",
  "description": "An opinionated template for using React and Redux.",
  "version": "0.2.0",
  "keywords": [
    "react",
    "redux"
  ],
  "scripts": {
    "build": "webpack --progress --config config/webpack.build.config.js",
    "dev": "webpack-dev-server --progress --colors --inline --hot --config config/webpack.dev.config.js",
    "test": "mocha --require src/test/setup.js --watch --compilers js:babel-register --recursive src/test/"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-template": "^5.0.0",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "sinon": "^1.17.5",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
